{
  "name": "Twitter Bot Checker",
  "description": "Extension for live checking if Twitter account can be bot",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": [
    "https://twitter.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}